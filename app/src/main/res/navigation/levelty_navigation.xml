            android:id="@+id/action_dayPersonalTasksDialogFragment_to_editTaskFragment"
            app:destination="@id/editTaskFragment" />
        <action
            android:id="@+id/action_dayPersonalTasksDialogFragment_to_editTaskFragment2"
            app:destination="@id/editTaskFragment" />
    </dialog>
